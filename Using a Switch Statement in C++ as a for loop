char grade = 'B';
switch (grade) {
    case 'A':
        cout << "Excellent" << endl;
        break;
    case 'B':
        cout << "Good" << endl;
        break;
    case 'C':
        cout << "Average" << endl;
        break;
    case 'D':
        cout << "Poor" << endl;
        break;
    default:
        cout << "Invalid grade" << endl;
}
